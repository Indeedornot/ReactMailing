{
	"name": "todoer",
	"version": "0.0.0",
	"scripts": {
		"dev": "concurrently \"vite\" \"nodemon ./server/server.ts\"",
		"build": "tsc && vite build",
		"serve": "vite preview",
		"preview": "vite",
		"lint:fix": "eslint client --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
		"lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
		"lint": "yarn lint:format && yarn lint:fix ",
		"type-check": "tsc",
		"storybook": "start-storybook -p 6006 -s public",
		"build-storybook": "build-storybook -s public"
	},
	"dependencies": {
		"@babel/core": "^7.19.3",
		"@babel/preset-env": "^7.19.4",
		"@emotion/babel-plugin": "^11.10.2",
		"@emotion/react": "^11.10.4",
		"@emotion/serialize": "^1.1.0",
		"@emotion/styled": "^11.10.4",
		"@types/react-virtualized-auto-sizer": "^1.0.1",
		"@types/react-window-infinite-loader": "^1.0.6",
		"classnames": "^2.3.2",
		"concurrently": "^7.4.0",
		"cors": "^2.8.5",
		"dayjs": "^1.11.5",
		"express": "^4.18.2",
		"imapflow": "^1.0.112",
		"mailparser": "^3.5.0",
		"nodemon": "^2.0.20",
		"react": "18.2.0",
		"react-collapsed": "^3.4.0",
		"react-dom": "18.2.0",
		"react-icons": "^4.6.0",
		"react-refresh": "^0.14.0",
		"react-spring": "^9.5.5",
		"react-virtuoso": "^3.1.1",
		"require-from-string": "^2.0.2",
		"ts-node": "^10.9.1"
	},
	"devDependencies": {
		"@emotion/babel-plugin-jsx-pragmatic": "^0.2.0",
		"@emotion/eslint-plugin": "^11.10.0",
		"@faker-js/faker": "^7.6.0",
		"@mdx-js/react": "2.1.5",
		"@storybook/addon-actions": "^6.5.12",
		"@storybook/addon-backgrounds": "6.5.12",
		"@storybook/addon-docs": "^6.5.12",
		"@storybook/addon-essentials": "^6.5.12",
		"@storybook/addon-interactions": "^6.5.12",
		"@storybook/addon-links": "^6.5.12",
		"@storybook/addon-measure": "6.5.12",
		"@storybook/addon-outline": "6.5.12",
		"@storybook/addon-postcss": "^2.0.0",
		"@storybook/addons": "^6.5.12",
		"@storybook/api": "^6.5.12",
		"@storybook/builder-vite": "^0.2.4",
		"@storybook/channel-postmessage": "^6.5.12",
		"@storybook/channel-websocket": "^6.5.12",
		"@storybook/client-api": "6.5.12",
		"@storybook/client-logger": "6.5.12",
		"@storybook/components": "^6.5.12",
		"@storybook/core-common": "6.5.12",
		"@storybook/core-events": "^6.5.12",
		"@storybook/mdx2-csf": "^0.0.3",
		"@storybook/node-logger": "^6.5.12",
		"@storybook/preset-create-react-app": "^4.1.2",
		"@storybook/preview-web": "^6.5.12",
		"@storybook/react": "^6.5.12",
		"@storybook/testing-library": "^0.0.13",
		"@storybook/theming": "^6.5.12",
		"@types/imapflow": "^1.0.11",
		"@types/mailparser": "^3.4.0",
		"@types/node": "^18.11.0",
		"@types/react": "^18.0.21",
		"@types/react-dom": "^18.0.6",
		"@typescript-eslint/eslint-plugin": "^5.40.0",
		"@typescript-eslint/parser": "^5.40.0",
		"@vitejs/plugin-react": "^2.1.0",
		"autoprefixer": "^10.4.12",
		"babel-plugin-macros": "^3.1.0",
		"eslint": "8.25.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.6.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.31.10",
		"eslint-plugin-storybook": "^0.6.6",
		"postcss": "^8.4.18",
		"pre-commit": "^1.2.2",
		"prettier": "^2.7.1",
		"sass": "^1.55.0",
		"storybook-dark-mode": "^1.1.2",
		"storybook-tailwind-dark-mode": "^1.0.15",
		"tailwindcss": "^3.1.8",
		"twin.macro": "^2.8.2",
		"typescript": "^4.8.4",
		"vite": "^3.1.8",
		"vite-tsconfig-paths": "^3.5.1"
	},
	"pre-commit": "lint",
	"license": "MIT",
	"eslintConfig": {
		"overrides": [
			{
				"files": [
					"**/*.stories.*"
				],
				"rules": {
					"import/no-anonymous-default-export": "off"
				}
			}
		]
	},
	"babelMacros": {
		"twin": {
			"preset": "emotion"
		}
	}
}
